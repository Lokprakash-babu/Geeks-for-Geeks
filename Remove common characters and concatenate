string concatenatedString(string s1, string s2) 
{ 
    string s3;
    for(int i=0; i<s1.length(); i++){
        if(s2.find(s1[i])!=-1){
            //do nothing
        }
        else{
            s3+=s1[i];
        }
    }
    
    for(int i=0; i<s2.length(); i++){
        if(s1.find(s2[i])!=-1){
            //
        }
        else{
            s3+=s2[i];
        }
    }
    return s3;
}
